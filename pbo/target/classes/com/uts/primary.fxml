<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.uts.PrimaryController">
    <children>
        <Label text="NOTE!" />
        <Label text="Setelah memilih soal, matikan program terlebih dahulu untuk memilih soal yang lain" />
        <Button fx:id="soal1Btn" text="SOAL 1" onAction="#switchToSoal1" />
        <Button fx:id="soal2Btn" text="SOAL 2" onAction="#switchToSoal2" />
        <Button fx:id="soal3Btn" text="SOAL 3" onAction="#switchToSoal3" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
